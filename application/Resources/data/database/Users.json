{
    "TableName": "Users",
    "Fields": {
        "UserId": {
            "type": "BIGINT",
            "constraint": 12,
            "unsigned": true,
            "auto_increment": true,
            "comment": "管理ID"
        },
        "AliasId": {
            "type": "CHAR",
            "constraint": 16,
            "null": false,
            "comment": "IDエリアス"
        },
        "Mail": {
            "type": "VARCHAR",
            "constraint": 256,
            "comment": "ログイン用メールアドレス"
        },
        "Password": {
            "type": "CHAR",
            "constraint": 60,
            "null": false,
            "comment": "ハッシュ済みパスワード"
        },
        "Nickname": {
            "type": "VARCHAR",
            "constraint": 30,
            "comment": "ニックネーム"
        },
        "LastLogin": {
            "type": "DATETIME",
            "comment": "最終ログイン日時"
        },
        "MailAuthed": {
            "type": "TINYINT",
            "constraint": 1,
            "default": 0,
            "comment": "メイル確認フラグ(0:未確認、1:確認済み)"
        },
        "LoginExclude": {
            "type": "TINYINT",
            "constraint": 1,
            "default": 0,
            "comment": "ログイン除外フラグ(0:未除外、1:除外)"
        }
    },
    "PrimaryKey": "UserId",
    "Index": [
        ["Mail"],
        ["AliasId"]
    ],
    "Attribute": {
        "ENGINE": "InnoDB",
        "COMMENT": "'ユーザ'"
    }
}
